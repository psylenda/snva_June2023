package coreJava2;
import java.util.Scanner;
public class Switch2 {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Enter char");
		char input = scanner.next().charAt(0);
		String season = "";
		String v = "vowel";
		String c = "consonant";
		switch(input) {
		case 1: 
			season = "Winter";
		case 2:
			season = "Winter";
		case 3:
			season = "Spring";
		case 4:
			season = "Spring";
		case 5:
			season = "Spring";
		case 6:
			season = "Summer";
		case 7:
			season = "Summer";
		case 8:
			season = "Summer";
		case 9:
			season = "Fall";
		case 10:
			season = "Fall";
		case 11:
			season = "Fall";
		case 12:
			season = "Winter";
			break;
		}
		System.out.println("The month is "+ month + " and the season is " + season + ".");
		scanner.close();
	}

}
